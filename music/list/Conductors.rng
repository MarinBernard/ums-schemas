<?xml version="1.0" encoding="UTF-8"?>
<grammar xmlns="http://relaxng.org/ns/structure/1.0">
	<define name="MLT_Conductors">
		<element name="conductors">
			<oneOrMore>
				<choice>
					<ref name="MCT_Conductor"/>
					<ref name="MRT_Conductor"/>
				</choice>
			</oneOrMore>
		</element>
	</define>
</grammar>